{
    "name": "betstore/laravel-dto",
    "description": "A reusable DTO class for Laravel projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneratorEVil",
            "email": "info@betstore.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/validation": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Betstore\\DTO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Betstore\\DTO\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}